{
    "model_type": "efficientnet_b0",
    "num_classes": 5,
    "batch_size": 16,
    "num_epochs": 100,
    "learning_rate": 0.0001,
    "optimizer": "adamw",
    "scheduler": "cosine",
    "loss_function": "focal",
    "weight_decay": 0.01,
    "patience": 15,
    "grad_clip": 0.5,
    "image_size": [
        224,
        224
    ],
    "test_size": 0.15,
    "val_size": 0.15,
    "num_workers": 2,
    "mixed_precision": true,
    "data_augmentation": {
        "rotation_range": 15,
        "zoom_range": 0.1,
        "brightness_range": 0.2,
        "contrast_range": 0.2,
        "horizontal_flip": true,
        "vertical_flip": false
    }
}